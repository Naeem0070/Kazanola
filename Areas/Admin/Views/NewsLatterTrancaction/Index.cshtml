@model IEnumerable<Kazanola.Models.NewsLetterTarncaction>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SubscriptionDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsSubscribed)
            </th>
            
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SubscriptionDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsSubscribed)
            </td>
       
            <td>
     
                <a asp-action="Details" asp-route-id="@item.NewsLetterTarncactionId">Details</a> 
               
            </td>
        </tr>
}
    </tbody>
</table>
